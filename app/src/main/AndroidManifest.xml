<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quiz">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logohg"
        android:label="@string/name_quiz"
        android:roundIcon="@drawable/logohg"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.DayNight.NoActionBar">
        <activity android:name=".activity.BiografiActivity"></activity>
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.HasilActivity" />
        <activity
            android:name=".activity.MateriActivity"
            android:parentActivityName=".activity.MainActivity" />
        <activity
            android:name=".activity.QuizActivity"
            android:parentActivityName=".activity.StartQuizActivity" />
        <activity
            android:name=".activity.ScoreActivity"
            android:parentActivityName=".activity.MainActivity" />
        <activity
            android:name=".activity.ResultActivity"
            android:parentActivityName=".activity.MainActivity" />
        <activity
            android:name=".activity.TimerActivity"
            android:parentActivityName=".activity.StartQuizActivity" />
        <activity
            android:name=".activity.StartQuizActivity"
            android:parentActivityName=".activity.MainActivity" />
    </application>

</manifest>